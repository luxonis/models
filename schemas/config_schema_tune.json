{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$ref": "#/definitions/blocks",
    "definitions": {
        "blocks": {
            "title": "Blocks",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "model": {
                    "$ref": "#/definitions/modelBlock"
                },
                "trainer": {
                    "$ref": "#/definitions/trainerBlock"
                },
                "logger": {
                    "$ref": "#/definitions/loggerBlock"
                },
                "dataset": {
                    "$ref": "#/definitions/datasetBlock"
                },
                "train": {
                    "$ref": "#/definitions/trainBlock"
                },
                "inferer": {
                    "$ref": "#/definitions/infererBlock"
                },
                "exporter": {
                    "$ref": "#/definitions/exporterBlock"
                },
                "tuner": {
                    "$ref": "#/definitions/tunerBlock"
                }
            },
            "required": [
                "model",
                "dataset",
                "tuner"
            ]
        },
        "modelBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "pretrained": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "params": {
                    "$ref": "#/definitions/nullParams"
                },
                "backbone": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "pretrained": {
                            "type": [
                                "null",
                                "string"
                            ]
                        },
                        "params": {
                            "$ref": "#/definitions/nullParams"
                        }
                    },
                    "required": [
                        "name"
                    ]
                },
                "neck": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "params": {
                            "$ref": "#/definitions/nullParams"
                        }
                    },
                    "required": [
                        "name"
                    ]
                },
                "heads": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/modelHead"
                    },
                    "minItems": 1
                },
                "additional_heads": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/modelHead"
                    },
                    "minItems": 1
                }
            },
            "if": {
                "properties": {
                    "type": {
                        "type": "null"
                    }
                }
            },
            "then": {
                "required": [
                    "backbone",
                    "heads"
                ]
            }
        },
        "trainerBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "accelerator": {
                    "type": "string",
                    "default": "auto"
                },
                "devices": {
                    "type": "string",
                    "default": "auto"
                },
                "strategy": {
                    "type": "string",
                    "default": "auto"
                },
                "num_sanity_val_steps": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 2,
                    "default": 2
                },
                "profiler": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "default": null
                },
                "verbose": {
                    "type": "boolean",
                    "default": true
                }
            }
        },
        "loggerBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "project_name": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "default": null
                },
                "project_id": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "default": null
                },
                "run_name": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "default": null
                },
                "run_id": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "default": null
                },
                "save_directory": {
                    "type": "string",
                    "default": "output"
                },
                "is_tensorboard": {
                    "type": "boolean",
                    "default": true
                },
                "is_wandb": {
                    "type": "boolean",
                    "default": false
                },
                "wandb_entity": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "default": null
                },
                "is_mlflow": {
                    "type": "boolean",
                    "default": false
                },
                "logged_hyperparams": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": [
                        "train.epochs",
                        "train.batch_size"
                    ]
                }
            }
        },
        "datasetBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "team_id": {
                    "type": "string"
                },
                "dataset_id": {
                    "type": "string"
                },
                "bucket_type": {
                    "type": "string",
                    "enum": [
                        "local",
                        "aws"
                    ],
                    "default": "local"
                },
                "override_bucket_type": {
                    "type": "boolean",
                    "default": false
                },
                "train_view": {
                    "type": "string",
                    "default": "train"
                },
                "val_view": {
                    "type": "string",
                    "default": "val"
                },
                "test_view": {
                    "type": "string",
                    "default": "test"
                }
            },
            "required": [
                "team_id",
                "dataset_id"
            ]
        },
        "trainBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "preprocessing": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "train_image_size": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            },
                            "minItems": 2,
                            "maxItems": 2,
                            "default": [
                                256,
                                256
                            ]
                        },
                        "keep_aspect_ratio": {
                            "type": "boolean",
                            "default": true
                        },
                        "train_rgb": {
                            "type": "boolean",
                            "default": true
                        },
                        "normalize": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "active": {
                                    "type": "boolean",
                                    "default": true
                                },
                                "params": {
                                    "$ref": "#/definitions/nullParams"
                                }
                            }
                        },
                        "augmentations": {
                            "type": [
                                "null",
                                "array"
                            ],
                            "default": null
                        }
                    }
                },
                "batch_size": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 64,
                    "default": 32
                },
                "accumulate_grad_batches": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 4,
                    "default": 1
                },
                "use_weighted_sampler": {
                    "type": "boolean",
                    "default": "false"
                },
                "epochs": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 100,
                    "default": 100
                },
                "num_workers": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 16,
                    "default": 2
                },
                "train_metrics_interval": {
                    "type": "integer",
                    "minimum": -1,
                    "default": -1
                },
                "validation_interval": {
                    "type": "integer",
                    "minimum": 1,
                    "default": 1
                },
                "num_log_images": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 4,
                    "default": 4
                },
                "skip_last_batch": {
                    "type": "boolean",
                    "default": true
                },
                "main_head_index": {
                    "type": "integer",
                    "default": 0
                },
                "use_rich_text": {
                    "type": "boolean",
                    "default": true
                },
                "callbacks": {
                    "type": "object",
                    "additionalItems": false,
                    "properties": {
                        "use_device_stats_monitor": {
                            "type": "boolean",
                            "default": false
                        },
                        "model_checkpoint": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "save_top_k": {
                                    "type": "integer",
                                    "minimum": 1,
                                    "maximum": 3,
                                    "default": 3
                                }
                            }
                        },
                        "early_stopping": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "active": {
                                    "type": "boolean",
                                    "default": true
                                },
                                "monitor": {
                                    "type": "string",
                                    "default": "val_loss/loss"
                                },
                                "mode": {
                                    "type": "string",
                                    "enum": [
                                        "max",
                                        "min"
                                    ],
                                    "default": "min"
                                },
                                "patience": {
                                    "type": "integer",
                                    "minimum": 1,
                                    "default": 5
                                },
                                "verbose": {
                                    "type": "boolean",
                                    "default": true
                                }
                            }
                        }
                    }
                },
                "optimizers": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "optimizer": {
                            "type": "object",
                            "additionalItems": false,
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "default": "Adam"
                                }
                            },
                            "params": {
                                "$ref": "#/definitions/nullParams"
                            }
                        },
                        "scheduler": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "default": "ConstantLR"
                                },
                                "params": {
                                    "$ref": "#/definitions/nullParams"
                                }
                            }
                        }
                    }
                },
                "freeze_modules": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "backbone": {
                            "type": "boolean",
                            "default": false
                        },
                        "neck": {
                            "type": "boolean",
                            "default": false
                        },
                        "heads": {
                            "type": "array",
                            "items": {
                                "type": "boolean"
                            },
                            "default": [
                                false
                            ]
                        }
                    }
                },
                "losses": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "log_sub_losses": {
                            "type": "boolean",
                            "default": false
                        },
                        "weights": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            },
                            "default": [
                                1,
                                1
                            ]
                        }
                    }
                }
            }
        },
        "infererBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "dataset_view": {
                    "type": "string",
                    "default": "val"
                }
            }
        },
        "exporterBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "export_weights": {
                    "type": "string"
                },
                "export_save_directory": {
                    "type": "string",
                    "default": "output_export"
                },
                "export_image_size": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    },
                    "minItems": 2,
                    "maxItems": 2,
                    "default": [
                        256,
                        256
                    ]
                },
                "export_model_name": {
                    "type": "string",
                    "default": "model"
                },
                "data_type": {
                    "type": "string",
                    "default": "FP16"
                },
                "reverse_input_channels": {
                    "type": "boolean",
                    "default": true
                },
                "scale_values": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    },
                    "minItems": 3,
                    "maxItems": 3,
                    "default": [
                        58.395,
                        57.120,
                        57.375
                    ]
                },
                "mean_values": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    },
                    "minItems": 3,
                    "maxItems": 3,
                    "default": [
                        123.675,
                        116.28,
                        103.53
                    ]
                },
                "onnx": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "opset_version": {
                            "type": "integer",
                            "default": 12
                        },
                        "dynamic_axes": {
                            "type": [
                                "null",
                                "object"
                            ],
                            "default": null
                        }
                    }
                },
                "openvino": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "active": {
                            "type": "boolean",
                            "default": false
                        }
                    }
                },
                "blobconverter": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "active": {
                            "type": "boolean",
                            "default": false
                        },
                        "shaves": {
                            "type": "integer",
                            "minimum": 1,
                            "maximum": 10,
                            "default": 6
                        }
                    }
                },
                "s3_upload": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "active": {
                            "type": "boolean",
                            "default": false
                        },
                        "bucket": {
                            "type": [
                                "null",
                                "string"
                            ]
                        },
                        "upload_directory": {
                            "type": [
                                "null",
                                "string"
                            ]
                        }
                    },
                    "if": {
                        "properties": {
                            "active": {
                                "const": true
                            }
                        }
                    },
                    "then": {
                        "properties": {
                            "bucket": {
                                "type": "string"
                            },
                            "upload_directory": {
                                "type": [
                                    "string"
                                ]
                            }
                        },
                        "required": [
                            "bucket",
                            "upload_directory"
                        ]
                    }
                }
            },
            "required": [
                "export_weights"
            ]
        },
        "tunerBlock": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "study_name": {
                    "type": "string",
                    "default": "test-study"
                },
                "use_pruner": {
                    "type": "boolean",
                    "default": true
                },
                "n_trials": {
                    "type": "integer",
                    "minimum": 1,
                    "default": 1
                },
                "timeout": {
                    "type": [
                        "null",
                        "integer"
                    ],
                    "default": null
                },
                "storage": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "active": {
                            "type": "boolean",
                            "default": true
                        },
                        "type": {
                            "type": "string",
                            "enum": [
                                "local",
                                "remote"
                            ],
                            "default": "remote"
                        },
                        "if": {
                            "properties": {
                                "active": {
                                    "const": true
                                }
                            }
                        },
                        "then": {
                            "required": [
                                "type"
                            ]
                        }
                    }
                },
                "params": {
                    "$ref": "#/definitions/nullParams"
                }
            },
            "required": [
                "params"
            ]
        },
        "nullParams": {
            "type": [
                "null",
                "object"
            ],
            "default": null
        },
        "modelHead": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "params": {
                    "$ref": "#/definitions/nullParams"
                },
                "loss": {
                    "type": "object",
                    "additionalItems": false,
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "params": {
                            "$ref": "#/definitions/nullParams"
                        }
                    },
                    "required": [
                        "name"
                    ]
                }
            },
            "required": [
                "name",
                "loss"
            ]
        }
    }
}